<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel="tests" name="Suite">

	<listeners>
		<listener class-name="seleniumTraining.TestComponents.Listeners" />
	</listeners>

	<test thread-count="5" name="submit order Test"> <!-- thread count is  how
		many methods we want to execute at once -->
		<classes>
			<class name="seleniumTraining.tests.SubmitOrderTest" />
		</classes>
	</test> <!--
	Test -->
	<test thread-count="5" name="Error validation Test">
		<classes>
			<class name="seleniumTraining.tests.ErrorValidationTest" />
			
		</classes>
	</test> <!--
	Test -->
</suite> <!-- 
Suite -->
